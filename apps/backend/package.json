{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=development bun run --watch ./src/index.ts",
    "start": "NODE_ENV=production bun run ./dist/index.js",
    "build": "NODE_ENV=production bun run ./scripts/build.ts",
    "type-check": "tsc --noEmit",
    "lint": "bunx biome lint",
    "------------------": "",
    "db:generate": "bun run drizzle-kit generate",
    "db:migrate": "bun run drizzle-kit migrate",
    "db:studio": "bun run ./scripts/studio.ts --target=$1 --mode=$2",
    "db:pull": "bun run drizzle-kit pull",
    "db:push": "bun run drizzle-kit push",
    "-------------------": "",
    "build:prod": "make build",
    "build:tag": "make tag",
    "build:push": "make push",
    "release": "make build && make tag && make push"
  },
  "dependencies": {
    "@elysiajs/bearer": "1.4.1",
    "@elysiajs/cors": "1.4.0",
    "@elysiajs/openapi": "1.4.11",
    "@elysiajs/server-timing": "1.4.0",
    "@nats-io/jetstream": "3.2.0",
    "@nats-io/nats-core": "3.2.0",
    "@nats-io/transport-node": "3.2.0",
    "@oslojs/crypto": "1.0.1",
    "@oslojs/encoding": "1.1.0",
    "@repo/shared": "workspace:*",
    "@tqman/nice-logger": "1.1.1",
    "bcryptjs": "3.0.2",
    "bowser": "2.13.1",
    "canvas": "3.2.0",
    "consola": "3.4.2",
    "croner": "9.1.0",
    "dayjs": "1.11.11",
    "drizzle-orm": "0.44.2",
    "elysia": "1.4.16",
    "elysia-http-status-code": "1.0.9",
    "elysia-ip": "1.0.10",
    "elysia-prometheus": "1.0.0",
    "elysia-rate-limit": "4.4.0",
    "gramio": "0.4.9",
    "heleket-api-sdk": "3.0.0",
    "ioredis": "5.6.1",
    "ky": "1.14.0",
    "kysely": "0.28.2",
    "kysely-bun-sqlite": "0.4.0",
